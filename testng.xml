<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="4" parallel="tests" name="DSAlgoSuite">
	<listeners>
		<listener class-name="com.dsalgo.listeners.TestNgListener"></listener>
	</listeners>
	<test name="ChromeTest">
		<parameter name="browser" value="chrome"></parameter>
		<packages>
			<package name="com.dsalgo.tests"></package>
		</packages>
	</test> <!-- Test -->
	<test name="EdgeTest">
		<parameter name="browser" value="edge"></parameter>
		<packages>
			<package name="com.dsalgo.tests"></package>
		</packages>
	</test> <!-- Test -->
</suite> <!-- Suite -->